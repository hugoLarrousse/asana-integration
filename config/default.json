{
  "requestRetry": {
    "maxAttempts": 3,
    "retryDelay": 100
  },
  "grantType": {
    "credentials": "authorization_code",
    "refreshToken": "refresh_token"
  },
  "env": "development",
  "h7Url": "http://localhost:3333",
  "triggerName": ["h7OpportunityTrigger", "h7EventTrigger", "h7TaskTrigger", "h7AccountTrigger", "h7UserTrigger"],
  "asanaApi": {
    "baseUrl": "https://app.asana.com",
    "oauthTokenPath": "/-/oauth_token",
    "usersPath": "/api/1.0/users"
  },
  "tokenExpiresIn": 3000000
}