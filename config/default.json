{
  "requestRetry": {
    "maxAttempts": 3,
    "retryDelay": 100
  },
  "env": "development",
  "h7Url": "http://localhost:3333",
  "asanaApi": {
    "baseUrl": "https://app.asana.com",
    "oauthTokenPath": "/-/oauth_token",
    "usersPath": "/api/1.0/users",
    "workspacesPath": "/api/1.0/workspaces",
    "projectsPath": "/api/1.0/projects",
    "taskPath": "/api/1.0/tasks",
    "webhookPath": "/api/1.0/webhooks",
    "version": "/api/1.0"
  },
  "tokenExpiresIn": 2000000
}